  <title>Wi-Move - Login</title>
  <link rel="stylesheet" href="css/master.css" type="text/css" />
  <link rel="stylesheet" href="css/mobile.css" type="text/css" />
  <script src="js/mobile.js" type="text/javascript"></script>
</head>

<body>
  <div id="containerBig">
    <div id="wrapperAuto">
      <div id="loginbox" style="width:100%%;height:100%%;padding:0px;margin:0px;">
        <div id="menulogo" class="menurow"><img src="images/logo_fiwifi.png" /></div>

        <form method="post" name="form" action="%s" id="form">

			  <input type="hidden" name="mac" value="%s">
			  <input type="hidden" name="ip" value="%s">
			  <input type="hidden" name="redirect" value="%s">
			  <input type="hidden" name="gateway" value="%s">
			  <input type="hidden" name="timeout" value="%s">
			  <input type="hidden" name="token" value="%s">
			  <input type="hidden" name="ap" value="%s">
			  <input type="hidden" name="realm" value="auth_service">

          <div id="page-wrap">
            <div id="inside">
              <div id="left-sidebar">
                <ul>
                  <li>
						  <!--
                    <p>Seleziona il cuircuito presso cui si Ã¨ registrati e inserire le credenziali</p>
						  -->
                    <p>Inserire le credenziali</p>
                  </li>
                </ul><br />
                <br />
                <br />
					 <!--
                <select id="realm" name="realm">
                  <option value="10_piazze">
                    10 Piazze
                  </option>

                  <option value="lineacomune">
                    Linea Comune
                  </option>

                  <option value="paas">
                    PAAS
                  </option>
                </select>
					 -->
              </div>

              <div id="main-content">
                <br />
                <br />
              </div>

              <p>Login (numero del cellulare):</p>

              <p><input class="login_input" type="text" name="uid" /></p>

              <p>Password:</p>

              <p><input class="login_input" type="password" name="pass" /></p>

              <p><input id="login_button" type="submit" name="submit" value="Entra" /></p>
            </div><br />
            <br />
          </div>
        </form>

        <div style="clear: both;"></div>

        <div id="footer"></div>
      </div>

      <div style="clear: both;"></div>
    </div>
  </div>
</body>
</html>
