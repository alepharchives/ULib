#!/bin/sh

. ../.function

start_msg uclient

rm -f uclient.log web_server.log \
		out/uclient.out err/uclient.err \
      out/userver_tcp.out err/userver_tcp.err \
		trace.*uclient*.[0-9]* object.*uclient*.[0-9]* \
		trace.*userver_tcp*.[0-9]* object.*userver_tcp*.[0-9]*

#UTRACE="0 10M 0"
#UOBJDUMP="0 100k 10"
#USIMERR="error.sim"
 export UTRACE UOBJDUMP USIMERR

DIR_CMD="../../examples/userver"

#STRACE=$TRUSS
start_prg_background userver_tcp -c web_server.cfg

$SLEEP
DIR_CMD="../../examples/uclient"

start_prg uclient -u server.log -c uclient.cfg http://127.0.0.1/cgi-bin/uploader.sh

diff server.log uploads/server.log
RESULT=$?

$SLEEP
kill_prg userver_tcp TERM

# Test against expected output
test_output_diff uclient
