#!/bin/sh

##
# dump.fw: show the status of all iptables.
##

IPTABLES=/usr/sbin/iptables

echo ">>>>> NAT table: <<<<<"
echo
$IPTABLES -t nat -L -n
echo
echo
echo ">>>>> Mangle table: <<<<<"
echo
$IPTABLES -t mangle -L -n
echo
echo
echo ">>>>> Filter table: <<<<<"
echo
$IPTABLES -t filter -L -n

